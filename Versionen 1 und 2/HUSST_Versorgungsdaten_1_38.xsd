  <xs:complexType name="Ortspunkttypen_Type">
    <xs:annotation>
      <xs:appinfo>
        <primekey>
          <field name="ID_OrtsPunktTyp"/>
        </primekey>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
