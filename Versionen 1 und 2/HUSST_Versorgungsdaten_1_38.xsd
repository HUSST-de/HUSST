          <xs:documentation>Linienhauptweg</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ID_Weg_Rueck" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Linienhauptweg Rückrichtung
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Bezeichnung" type="tns:PrintableString80" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Konzessionaer" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Bediengebiet" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="Wege_Type">
    <xs:annotation>
      <xs:documentation>
        Fahrtnummer, Kursnummer
      </xs:documentation>
      <xs:appinfo>
        <primekey>
          <field name="ID_Weg"/>
          <field name="ID_Zeitraum"/>
        </primekey>
        <schema name="Angebot" nillable="true"/>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="Deaktiviert" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Zeitraum" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_Weg" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Eindeutige Nummer des Weges.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Bezeichnung" type="tns:PrintableString80" nillable="true" minOccurs="0" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="Wegpositionen_Type">
    <xs:annotation>
      <xs:documentation>
        Ortsposition innerhalb der Fahrt
      </xs:documentation>
      <xs:appinfo>
        <primekey>
          <field name="ID_Weg"/>
          <field name="SortOrder"/>
          <field name="ID_Zeitraum"/>
        </primekey>
        <index name="Idx_WP_OP">
          <field name="ID_OrtsPunktTyp"/>
          <field name="ID_OrtsNummer"/>
          <field name="ID_Weg"/>
        </index>
        <schema name="Angebot" nillable="true"/>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="Deaktiviert" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Zeitraum" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_Weg" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="SortOrder" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_OrtsPunktTyp" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_OrtsNummer" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Bediengebiet" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Entfernung" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Entfernung vom vorhergehenden Ortspunkt in Metern
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Fangbereich" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            optionaler Fangbereich der Wegposition in Metern für eine
            GPS Ortung - überschreibt eine entsprechende Angabe bei der
            Ortspunktdefinition
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="Wegpositionenzeit_Type">
    <xs:annotation>
      <xs:documentation>
        Fahr- und Haltezeiten einer Wegposition für verschiedene
        Fahr-/Haltezeitgruppen
      </xs:documentation>
      <xs:appinfo>
        <schema name="Angebot" nillable="true"/>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="Deaktiviert" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Zeitraum" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_Weg" type="tns:INT4" maxOccurs="1" minOccurs="1" nillable="false">
        <xs:annotation>
          <xs:documentation>
            ID_Weg bildet mit SortOrder den Fremdschlüssel zur
            Wegposition, der sie zugeordnet sind.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SortOrder" type="tns:INT4" maxOccurs="1" minOccurs="1" nillable="false">
      </xs:element>
      <xs:element name="ID_Wegzeittyp" type="tns:INT4" maxOccurs="1" minOccurs="1" nillable="false">
        <xs:annotation>
          <xs:documentation>
            Wegzeittyp definiert die Art von Wegzeit, die hier
            dokumentiert wird: 1=Fahrzeit, 2=Haltezeit.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Zeitgruppe" type="tns:INT4" maxOccurs="1" minOccurs="1" nillable="false">
        <xs:annotation>
          <xs:documentation>
            Nummer der Haltezeitgruppe bzw. der
            Fahrzeitgruppe, zu der die angegebene Wegpositionenzeit
            gehört.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Dauer" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>Dauer der Wegpositionszeit in Sekunden.
            Bei Fahrzeiten ist das die Dauer bis zum Erreichen der nächsten
            Wegposition,
            bei Haltezeiten ist das die Aufenthaltsdauer bis zur Abfahrt an
            dieser Wegposition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="Zielanzeigetext_Type">
