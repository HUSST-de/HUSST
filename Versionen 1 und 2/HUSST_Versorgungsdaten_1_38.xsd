          <xs:documentation>
            Tarifart (siehe Hilfstabelle Tarifart)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="Sorten_Type">
    <xs:annotation>
      <xs:documentation>
        Artikel/Produkte/Fahrscheintypen
      </xs:documentation>
      <xs:appinfo>
        <primekey>
          <field name="ID_Sorte"/>
          <field name="ID_Zeitraum"/>
        </primekey>
        <index name="Idx_So_Tarifgebiet">
          <field name="ID_TarifGebiet"/>
          <field name="ID_Zeitraum"/>
        </index>
        <index name="Idx_So_Tarifgeber">
          <field name="SorteTarifgeber"/>
          <field name="ID_Zeitraum"/>
        </index>
        <index name="Idx_So_Komfortklasse">
          <field name="Komfortklasse"/>
          <field name="ID_Zeitraum"/>
        </index>
        <index name="Idx_So_Rabattklasse">
          <field name="Rabattklasse"/>
          <field name="ID_Zeitraum"/>
        </index>
        <index name="Idx_So_Fahrgasttyp">
          <field name="Fahrgasttyp"/>
          <field name="ID_Zeitraum"/>
        </index>
        <schema name="Tarif" nillable="false"/>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="Deaktiviert" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Zeitraum" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_Sorte" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Nummer der Sorte, eindeutig über alle
            Tarifgebiete und Tarifversionen
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ID_TarifGebiet" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>
      <xs:element name="ID_Sortenklasse" type="tns:INT4" nillable="false" maxOccurs="1" minOccurs="1"/>

      <xs:element name="SorteTarifgeber" nillable="true" minOccurs="0" maxOccurs="1" type="tns:PrintableString80">
        <xs:annotation>
          <xs:documentation>
            Schlüssel des Tarifgebers für diese Sorte
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BezeichnungKurz" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Identifikation / optional Druck
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BezeichnungLang" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Identifikation / optional Druck
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AnzeigeKurz" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Produktanwahl
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AnzeigeLang" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Produktanwahl / Warenkorb
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="KundendisplayKurz" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Kundendisplay
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="KundendisplayLang" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Kundendisplay
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BelegdruckKurz" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Belegdruck
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BelegdruckLang" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Verwendung: Belegdruck
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Druckoption" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            wenn die Druckoption nicht angegeben ist, wird sie
            als Wert=0 interpretiert. 0:Druck über Layout bzw.
            LayoutBeleg 1:Druck über ein zum Verkaufszeitpunkt
            zugeordnetes Hauptprodukt 2:Druck über ein zum
            Verkaufszeitpunkt zugeordnetes Hauptprodukt oder
            über Layout bzw. LayoutBeleg, wenn kein Hauptprodukt
            zugeordnet wurde
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Layout" type="tns:blobString" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Layoutdefinition für den Ausdruck des Produktes
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LayoutBeleg" type="tns:blobString" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Layouthinweise für den Ausdruck eines Einzelbeleges.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BelegAnzahl" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Wieviele Einzelbelege sind für dieses Produkt zu
            erstellen? 0..n
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Sammelbeleg" type="tns:SammelbelegType" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Soll das Produkt auf einem Sammelbeleg aufgeführt
            werden? Ja/Nein
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Vertriebswege" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Bitfeld Vertriebswege
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ID_MwSt" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ZahlungsArt" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Zahlungsart 1 Byte:

            0x00..0x0f: Barzahlung oder barzahlungsäquivalente
            Zahlung: 0x00 = Barzahlung, 0x0f = allgemeine
            barzahlungsäquivalente Zahlung 0x10..0x1f:
            Geldkartenzahlung: 0x10 = GeldKarte, 0x1f =
            allgemeine Geldkartenzahlung 0x20..0x2f:
            ec-Cash-Varianten: 0x20 = ec-Cash 0x30..0x3f:
            Gutscheinzahlung: 0x3f = allgemeine Gutscheinzahlung
            0x40..0x4f: Pre-Paid-Zahlung (nicht
            bargeldäquivalent): 0x40..0x47 = kundeneigene
            Pre-Paid-Zahlungen(z.B. LEGIC), 0x48 = DING-Card,
            0x49 = BODO-Card, 0x4f = allgemeine Pre-Paid-Zahlung
            0x80..0x8f: Lastschriftzahlung: 0x80 = ec
            Lastschrift, 0x8f = allgemeine Lastschriftzahlung
            0x90..0x9f: Rechnungszahlung: 0x9f = allgemeine
            Zahlung 0xa0..0xaf: Kreditkartenzahlung: 0xa0 =
            Visa, 0xa1 = Euro-/MasterCard, 0xa2 = American
            Express, 0xa3 = Diners Club, 0xa4 = Maestro, 0xaf =
            allgemeine Kreditkartenzahlung 0xe0..0xef: sonstige
            Post-Paid-Zahlungen: 0xe0 = KA-BOB-Card, 0xef =
            allgemeine Post-Paid-Zahlung 0xf0..0xff: Sonstige
            Zahlung: 0xff = allgemeine sonstigeZahlung
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Komfortklasse" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            (z.B. 1.Kl., 2.Kl., ...) Verweis auf
            Sortenattributgruppen.AttributText
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Rabattklasse" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            zum Beispiel: Bahncard/Bahncard 25/SH-Card Die
            Rabattklassen schließen sich gegenseitig aus.
            Verweis auf Sortenattributgruppen.AttributText
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Fahrgasttyp" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            normal Kind Schüler Azubi Verweis auf
            Sortenattributgruppen.AttributText
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="SortenTyp" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            1=Tageskarte,2=Wochenkarte,3=Monatskarte,4=Halbjahreskarte,5=Jahreskarte,6=Mehrfahrten,0=Einzelkarte,7=Bargutschrift,8=AboEinzahlung,9=EBEEinzahlung
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Zusatzsorte" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            TRUE, wenn die Sorte eine Zusatzsorte ist, dann gibt
            es ein Objekt des Typs Zusatzsorten_Type
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Mindestpersonenanzahl" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Ab wievielen Personen darf der Fahrschein verkauft
            werden
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Hoechstpersonenanzahl" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Bis zu wievielen Personen darf der Fahrschein
            maximal verkauft werden
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="KdNummer" type="tns:PrintableString" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Definiert, ob und in welchem Format eine
            Kundennummer zu erfassen ist. Das Format definiert,
            wieviele und welche Zeichen erfasst werden können:
            a: -&gt; mit diesem Hinweis muss der Formatstring
            beginnen, er definiert die folgende Syntax ?=steht
            für ein beliebiges Zeichen #=steht für eine Zahl
            $=steht für eine beliebige Zahl oder ein beliebiger
            Buchstabe (ohne Sonderzeichen ohne Leerzeichen)
            Optionale Zeichen sind mit [] einzufassen Beispiel:
            a:##$$[??????] es müssen zwei Ziffern, gefolgt von
            zwei Ziffern oder Buchstaben gefolgt von bis zu 6
            beliebigen Zeichen eingegeben werden. a:#[#########]
            eine Zahl mit mindestens einer und max. 10 Ziffern
            a:[##########] eine beliebige Zahl mit max. 10
            Ziffern - Eingabe ist optional
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GueltigRegel" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            1=KW/KM/KJ (fest), 2=gleitend - nächste Einheit -1,
            3=Anzahl Tage/Monate
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GueltigParam" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            bei GRegel 3: Anzahl Tage/Monate
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="250"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GueltigText" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GueltigRegelEFS" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            1=KW/KM/KJ (fest), 2=gleitend - nächste Einheit -1,
            3=Anzahl Tage/Monate
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GueltigParamEFS" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            bei GRegel 3: Anzahl Tage/Monate
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="250"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Vorverkauf" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Wieviele Tage im Voraus verkaufbar
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Umschalttag" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            ab welchem Tag, wird der nächste feste Zeitraum
            angeboten (GRegel=1)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PreisuebergangVorverkauf" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Gibt an, wie lange die Sorte im Vorverkauf mit dem
            aktuellen Preis verkauft wird, obwohl ihre
            Gültigkeit schon im neuen Tarif liegt.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="250"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Preisfindung" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Hinweis zur Preisfindung 1=Festpreis,
            2=Relationsbezogen, 3=Relationsbezogen mit
            Preisstufenupgrade
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Preisabschlussberechnung" type="xs:string" maxOccurs="1" minOccurs="0" nillable="true">
        <xs:annotation>
          <xs:documentation>Definiert die Regel(n), nach der ein ermittelter
            Fahrscheinpreis am Ende noch einmal gegen generelle Preisnormen
            abgeglichen wird.
            Z.B.: Mindespreis, Rundung, etc...

            wenn keine Regel angegeben ist, gilt der ermittelte Preis

            Die Definition der Regeln ist in der Dokumentation hinterlegt.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AnzahlBerechtigungen" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Anzahl Fahrtberechtigungen bei SortenTyp 6
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AnzahlAbschnitte" type="tns:INT4" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Anzahl FSAbschnitte bei SortenTyp 6
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HinUndRueckfahrt" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="TarifInfo" type="tns:blobString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="Drucktext1" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Drucktext Verwendung ist abhängig von der
            Layoutdefinition
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="250"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Drucktext2" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
