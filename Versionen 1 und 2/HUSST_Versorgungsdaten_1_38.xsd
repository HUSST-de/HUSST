  <xs:complexType name="Tarifgebiete_Type">
    <xs:annotation>
      <xs:documentation>
        Tarifgebiet ist eine logische Einheit, die alle
        Tarifelemente eines Tarifgebers
        zusammenfasst
      </xs:documentation>
      <xs:appinfo>
        <primekey>
          <field name="ID_TarifGebiet"/>
          <field name="ID_Zeitraum"/>
        </primekey>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="Deaktiviert" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ID_Zeitraum" type="tns:INT4" nillable="false" minOccurs="1" maxOccurs="1"/>
      <xs:element name="ID_TarifGebiet" type="tns:INT4" nillable="false" minOccurs="1" maxOccurs="1"/>
      <xs:element name="BezeichnungKurz" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="20"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BezeichnungLang" type="tns:PrintableString80" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Erstellungsdatum" type="tns:DateTimeCompact" nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Version" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
            <xs:minLength value="0"/>
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>

