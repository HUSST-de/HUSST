      </xs:documentation>
      <xs:appinfo>
        <primekey>
          <field name="ID_TarifArt"/>
          <field name="ID_Zeitraum"/>
        </primekey>
        <schema name="Tarif" nillable="true"/>
      </xs:appinfo>
