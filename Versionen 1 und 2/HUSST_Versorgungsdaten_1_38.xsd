      <xs:element name="Fahreranzeige" type="tns:PrintableString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="FronttextOben" type="tns:PrintableString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="FronttextUnten" type="tns:PrintableString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="SeitetextOben" type="tns:PrintableString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="SeitetextUnten" type="tns:PrintableString" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
      <xs:element name="Sonderzeichen" type="tns:INT4" maxOccurs="1" minOccurs="0" nillable="true">
      </xs:element>
    </xs:all>
  </xs:complexType>

