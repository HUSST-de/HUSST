      </xs:element>
      <xs:element name="Anbindung" nillable="true" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>„V“=Vorlauf, „N“=Nachlauf, „H“=Hauptlauf</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
          <xs:restriction base="tns:Char">
            <xs:enumeration value="V"/>
            <xs:enumeration value="N"/>
            <xs:enumeration value="H"/>
          </xs:restriction>
