        </primekey>
      </xs:appinfo>
    </xs:annotation>
    <xs:all>
      <xs:element name="ID_RelCodeGruppenTyp" type="tns:INT4" nillable="false" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Bezeichnung" type="tns:PrintableString80" nillable="true" minOccurs="0" maxOccurs="1">
