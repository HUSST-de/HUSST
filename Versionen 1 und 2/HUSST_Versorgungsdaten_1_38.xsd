        <xs:annotation>
          <xs:documentation>ISO-Kurzbezeichnung z.B. EUR, USD, CHF,...
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PrintableString">
