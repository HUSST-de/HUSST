					<xs:documentation>Kennung des Tagesmerkmalelements</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ID_Tagesmerkmal" type="tpd:INT4" nillable="false" minOccurs="1" maxOccurs="1">
