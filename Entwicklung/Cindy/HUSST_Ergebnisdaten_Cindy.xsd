				</annotation>
			</element>
			<element name="Pruefer" type="string" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>
						Beteiligte Prüfer (Nummer)
					</documentation>
				</annotation>
			</element>
			<element name="Linie" type="int" minOccurs="0" maxOccurs="1">
