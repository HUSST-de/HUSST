			</element>
			<element name="Transaktionsdaten" type="husst:Any_Type" maxOccurs="1" minOccurs="1">
			</element>
			<choice>
				<element name="KAReferenz" type="husst:KAReferenz_Type" maxOccurs="1" minOccurs="0">
