				</xs:annotation>
			</xs:element>
			<xs:element name="KA_ProdNr" type="husstDV:INT4" nillable="true" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>KA Produkt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KA_Infotext" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="KA_Fahrgasttyp" type="husstDV:INT4" nillable="true" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>KA Fahrgasttyp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KA_Mitnahme" type="husstDV:INT4" nillable="true" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>KA Mitnahme</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KA_Verkehrsmittel" type="husstDV:INT4" nillable="true" minOccurs="0" maxOccurs="1">
				<xs:annotation>
