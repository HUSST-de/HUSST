				</xs:annotation>
			</xs:element>
			<xs:element name="BezeichnungKurz" type="xs:string" maxOccurs="1" minOccurs="0"/>
			<xs:element name="Hinweis" type="xs:string" minOccurs="0" maxOccurs="1">
