			</xs:documentation>
			<xs:appinfo>
				<api:primekey>
					<api:field name="ID_Sorte"/>
					<api:field name="ID_Zeitraum"/>
				</api:primekey>
				<api:schema name="Tarif"/>
			</xs:appinfo>
