			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Papierfahrschein / EFS (z.B. Chipkarte)
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Papierfahrschein / Handy Ticket
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
